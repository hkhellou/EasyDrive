<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_viajes">

    <fragment
        android:id="@+id/nav_viajes"
        android:name="com.example.easydrive10.principal.ui.viajes.ViajesFragment"
        android:label="@string/menu_viajes"
        tools:layout="@layout/fragment_viajes">
        <action
            android:id="@+id/action_nav_viajes_to_NuevoViajeFragment"
            app:destination="@id/NuevoViajeFragment"
            app:popUpTo="@id/NuevoViajeFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_gastos"
        android:name="com.example.easydrive10.principal.ui.gastos.GastosFragment"
        android:label="@string/menu_gastos"
        tools:layout="@layout/fragment_gastos" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.easydrive10.principal.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_viajes"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/NuevoViajeFragment"
        android:name="com.example.easydrive10.principal.ui.viajes.NuevoViajeFragment"
        android:label="fragment_nav_nuevo_viaje"
        tools:layout="@layout/fragment_nav_nuevo_viaje" />
</navigation>